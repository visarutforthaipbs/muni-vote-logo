	position: relative;
	overflow: hidden;
	padding: 10px;
	background-color: aliceblue;
}
.title-top strong h1{
	font-size: 25px;
	overflow: hidden;
	position: relative;
	color: chocolate;
}
.title-top p{
	font-size: 13.5px;
	color: #0000009c;
	display: block;
	position: relative;
}
#article .container{
	padding-right: 5px;
	padding-left: 5px;
	overflow: hidden;
}
#navbarSupportedContent{
	padding: -5px;
}
#navbarNavDropdown .nav-item{

	border-right: 1px solid rgba(0,0,0,0.1);
}
.navbar-dark .navbar-nav .nav-link {
	color: rgb(255, 255, 255);
}
